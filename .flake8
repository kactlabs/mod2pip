[flake8]
max-line-length = 100
exclude = 
    .git,
    __pycache__,
    build,
    dist,
    *.egg-info,
    .tox,
    .venv,
    venv,
    env,
    tests/_data*,
    tests/_invalid_data,
    docs/conf.py,
    ci_debug.py
ignore = 
    E501,
    W503,
    W504,
    W293,
    W291,
    E129,
    F541
